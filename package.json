{
  "name": "t3-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "prebuild": "yarn generate && yarn migrate-deploy",
    "build:1-next": "cross-env NODE_ENV=production next build",
    "build:2-server": "rm -rf build && tsc --project tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "build": "run-s build:*",
    "dev:wss": "cross-env NODE_ENV=development PORT=3001 tsx watch src/server/wss-dev-server.ts src/server/socket/** --tsconfig tsconfig.server.json",
    "dev:next": "next dev",
    "dev": "yarn migrate-dev && env-cmd run-p dev:*",
    "lint": "next lint",
    "generate": "prisma generate",
    "seed-dev": "prisma db seed -- --environment development",
    "seed-prod": "prisma db seed -- --environment production",
    "migrate-dev": "prisma migrate dev",
    "migrate-deploy": "prisma migrate deploy",
    "start": "cross-env NODE_ENV=production node dist/server/prod-server.js",
    "studio": "prisma studio",
    "postinstall": "yarn generate"
  },
  "prisma": {
    "schema": "./node_modules/@katitb2023/database/schema.prisma"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.7.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@google-cloud/storage": "^6.11.0",
    "@hookform/resolvers": "^3.1.1",
    "@katitb2023/database": "ssh://git@github.com:KATITB2023/database.git#0d4c8f316d0455b4b778704262744e1a637d53b2",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.40.0",
    "@opentelemetry/instrumentation": "^0.40.0",
    "@opentelemetry/resources": "^1.14.0",
    "@opentelemetry/sdk-trace-base": "^1.14.0",
    "@opentelemetry/sdk-trace-node": "^1.14.0",
    "@opentelemetry/semantic-conventions": "^1.14.0",
    "@prisma/client": "^5.0.0",
    "@prisma/instrumentation": "^5.0.0",
    "@socket.io/redis-streams-adapter": "^0.1.0",
    "@t3-oss/env-nextjs": "^0.3.1",
    "@tanstack/react-query": "^4.29.19",
    "@trpc/client": "^10.26.0",
    "@trpc/next": "^10.26.0",
