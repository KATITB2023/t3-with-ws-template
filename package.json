    "next": "^13.4.2",
    "next-auth": "^4.22.1",
    "node-cron": "^3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.0",
    "redis": "^4.6.7",
    "sanitize-filename": "^1.6.3",
    "sharp": "^0.32.1",
    "socket.io": "^4.7.1",
    "socket.io-adapter": "^2.5.2",
    "socket.io-client": "^4.7.1",
    "socket.io-parser": "^4.2.4",
    "superjson": "1.12.2",
    "trpc-panel": "^1.3.4",
    "ts-pattern": "^5.0.1",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
